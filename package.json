{
  "name": "travultr",
  "version": "0.0.5",
  "description": "A CLI client for Vultr",
  "main": "index.js",
  "scripts": {
    "test": "ava --verbose",
    "posttest": "xo"
  },
  "files": [
    "lib",
    "build",
    "README.md",
    "MANIFESTO.md"
  ],
  "bin": {
    "travultr": "./bin/travultr.js"
  },
  "engines": {
    "node": ">=7.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugomd/travultr.git"
  },
  "author": {
    "name": "Hugo Muller-Downing",
    "email": "npmjs@hugo.sx",
    "url": "hu.md"
  },
  "keywords": [
    "vultr",
    "api",
    "client",
    "cli"
  ],
  "xo": {
    "space": true,
    "rules": {
      "camelcase": 0,
      "promise/no-promise-in-callback": 0,
      "array-callback-return": 0,
      "curly": 0,
      "unicorn/no-process-exit": 0
    }
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hugomd/travultr/issues"
  },
  "homepage": "https://github.com/hugomd/travultr#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "commander": "git+https://github.com/tj/commander.js.git",
    "form-data": "^2.1.2",
    "fs-promise": "^2.0.0",
    "node-fetch": "^1.6.3",
    "prompt": "^1.0.0",
    "promptly": "^2.1.0",
    "superagent": "^3.5.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "xo": "^0.18.0"
  }
}
